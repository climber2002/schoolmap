(function() {

  <%# update breadcrumb -%>
  $("#ribbon").html("<%= escape_javascript(render partial: 'shared/breadcrumb') %>")

  if(currentAreaPolygon !== undefined) {
    currentAreaPolygon.setMap(null);
  }
  currentAreaPolygon = gabonPolygon;
  currentAreaPolygon.setMap(map);

  if(currentData !== undefined) {
    currentData.setMap(null);
  }
  currentData = provincesData;
  currentData.setMap(map);
  
  //Set center to garbon
  var gabon = new google.maps.LatLng(-0.6225172457730085, 11.785067468340143);
  map.setCenter(gabon);

  map.set('zoom', 7);

  // Load all schools
  $.getScript("<%= schools_path(format: :js) %>")
 })();